{
  "name": "@syncbase/web",
  "packageManager": "yarn@3.0.2",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"next,graphql\" -c \"bgBlack,bgMagenta\" \"next dev\" \"yarn gen --watch\"",
    "build": "yarn gen && next build",
    "start": "next start",
    "check-types": "yarn gen && tsc --noEmit",
    "gen": "graphql-codegen"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.2",
    "graphql": "^15.6.1",
    "next": "^11.1.2",
    "next-urql": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.5",
    "sharp": "^0.29.2",
    "urql": "^2.0.5",
    "zod": "^3.11.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.1",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-operations": "2.1.8",
    "@graphql-codegen/typescript-urql": "^3.2.2",
    "@types/node": "^16.11.5",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/tailwindcss": "^2.2.2",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
